{
  "name": "@behold/svelte",
  "version": "0.1.7",
  "description": "The official Behold Widget for Svelte",
  "keywords": [
    "Behold",
    "Svelte",
    "SDK",
    "Instagram",
    "widget",
    "embedded widget",
    "web components"
  ],
  "homepage": "https://behold.so",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeholdSocial/behold-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/BeholdSocial/behold-svelte/issues"
  },
  "author": "Griffin Johnston",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": ">=4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/kit": "^2.26.0",
    "@sveltejs/package": "^2.4.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "publint": "^0.3.12",
    "svelte": "^5.36.16",
    "svelte-check": "^4.3.0",
    "tslib": "^2.4.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.6"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
